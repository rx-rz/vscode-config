{
  "apc.electron": {
    //hide title bar
    "titleBarStyle": "hidden",
    "trafficLightPosition": {
      "x": 8,
      "y": 10
    }
  },
  "editor.lineHeight": 1.5,
  "apc.header": {
    "height": 30,
    "fontSize": 10
  },
  "apc.listRow": {
    "height": 30,
    "fontSize": 13,
    "lists": [
      "customview-tree",
      "results",
      "open-editors",
      "explorer-folders-view",
      "outline-tree",
      "scm-view",
      "debug-view-content",
      "debug-breakpoints",
      "tree"
    ]
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "Fira Code Regular",
  "glassit.alpha": 250,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSave": "onFocusChange",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "gitlens.ai.experimental.provider": "openai",
  "gitlens.ai.experimental.openai.model": "gpt-3.5-turbo-1106",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.enableSmartCommit": true,

  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 4,
  "editor.stickyScroll.enabled": true,
  "git.confirmSync": false,
  "workbench.iconTheme": "moxer-icons",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.settings.editor": "json",
  "breadcrumbs.enabled": false,
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.inlineSuggest.enabled": true,

  "apc.font.family": "Fira Code Regular",
  "apc.statusBar": {
    "position": "editor-bottom",
    "height": 20,
    "fontSize": 10
  },

  "terminal.integrated.fontFamily": "Fira Code Regular",
  "editor.fontLigatures": true,
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "itemSize": 60,
    "size": 60
  },
  "chat.editor.lineHeight": 2,
  "editor.lineNumbers": "off",
  "window.titleBarStyle": "native",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "terminal.integrated.env.windows": {},
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "phase",
  "workbench.sideBar.location": "right",
  "yaml.schemas": {
    "file:///c%3A/Users/Temiloluwa/.vscode/extensions/atlassian.atlascode-3.0.9/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.bitbucket.enabled": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": -1
}
